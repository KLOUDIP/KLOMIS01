<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_project_task_create_timesheet_view_form" model="ir.ui.view">
        <field name="name">project.task.create.timesheet.wizard.form.view</field>
        <field name="model">project.task.create.timesheet</field>
        <field name="inherit_id" ref="timesheet_grid.project_task_create_timesheet_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='description']" position="after">
                <field name="start_time" invisible="1"/>
                <field name="end_time" invisible="1"/>
            </xpath>
        </field>
    </record>

</odoo>