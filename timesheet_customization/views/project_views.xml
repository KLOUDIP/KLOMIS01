<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_task_form2_view" model="ir.ui.view">
        <field name="name">project.task.form.view</field>
        <field name="model">project.task</field>
        <field name="inherit_id" ref="project.view_task_form2"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='tag_ids']" position="after">
                <field name="real_start_time" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='timesheet_ids']//field[@name='name']" position="after">
                <field name="start_time" readonly="1"/>
                <field name="end_time" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>